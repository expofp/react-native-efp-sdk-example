{
  "expo": {
    "name": "expofp-sdk-rn-example",
    "slug": "expofp-sdk-rn-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expofpsdkrnexample",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expofpdev.expofpsdkrnexample",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to display it on the map and provide location-based services.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs your location to provide real-time tracking and location-based services.",
        "NSBluetoothAlwaysUsageDescription": "This app uses Bluetooth to improve location accuracy and discover nearby devices.",
        "NSBluetoothPeripheralUsageDescription": "This app uses Bluetooth to improve location accuracy and discover nearby devices.",
        "NSBonjourServiceTypes": [],
        "NSLocalNetworkUsageDescription": "This app uses local network to discover devices on your network.",
        "UIBackgroundModes": ["location", "bluetooth-central"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.expofpdev.expofpsdkrnexample"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
